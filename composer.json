{
    "name": "laravel/ranger",
    "description": "Laravel Ranger is a powerful introspection library for Laravel applications.",
    "keywords": [
        "laravel"
    ],
    "homepage": "https://github.com/laravel/ranger",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/ranger/issues",
        "source": "https://github.com/laravel/ranger"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "laravel/pint": "^1.22",
        "laravel/surveyor": "@dev",
        "nikic/php-parser": "^5.4",
        "phpstan/phpdoc-parser": "^2.1",
        "spatie/php-structure-discoverer": "^2.3"
    },
    "require-dev": {
        "inertiajs/inertia-laravel": "^2.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "phpstan/phpstan": "2.1.24"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Ranger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "workbench/app/",
            "Laravel\\Ranger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Ranger\\RangerServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../surveyor"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
